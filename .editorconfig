# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = space
intent_size = 2

# Indentation override for all JS 
[*.js]
indent_style = space
indent_size = 2

# Matches the exact files package.json
[{package.json}]
indent_style = space
indent_size = 2
